{
  "name": "n8n-nodes-guru-api",
  "version": "1.5.7",
  "description": "n8n Community Nodes for Digital Manager Guru API integration",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "guru",
    "digital-manager",
    "api",
    "contacts",
    "transactions",
    "subscriptions"
  ],
  "license": "MIT",
  "homepage": "https://github.com/matheuskuboadapta/guru-digital-manager-community-node#readme",
  "author": {
    "name": "Matheus Kubo",
    "email": "matheus@adaptaconsultoria.com.br"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matheuskuboadapta/guru-digital-manager-community-node.git"
  },
  "bugs": {
    "url": "https://github.com/matheuskuboadapta/guru-digital-manager-community-node/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint src/**/*.ts",
    "lintfix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js src/**/*.ts"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/GuruDigitalManager/GuruDigitalManager.node.js"
    ],
    "credentials": [
      "dist/credentials/GuruDigitalManagerApi.credentials.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.0.0",
    "gulp": "^4.0.2",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "dependencies": {}
}
